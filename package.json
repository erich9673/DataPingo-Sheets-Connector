    "frontend": "cd sheets-connector-app && npm start",
    "postinstall": "cd sheets-connector-backend && npm ci"
  },
  "keywords": [
    "google-sheets",
    "slack",
    "monitoring",
    "real-time",
    "datapingo",
    "automation",
    "webhooks",
    "notifications"
  ],
  "author": "DataPingo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/erichuang2003/DataPingo-Sheets-Connector.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
