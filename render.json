{
  "services": [
    {
      "type": "web",
      "name": "datapingo-sheets-connector",
      "env": "node",
      "region": "oregon",
      "plan": "starter",
      "buildCommand": "echo '=== RENDER BUILD START ===' && npm ci && echo '=== Installing backend deps ===' && cd sheets-connector-backend && npm ci && echo '=== RENDER BUILD COMPLETE ==='",
      "startCommand": "echo '=== RENDER START ===' && node server.js",
      "runtime": "node",
      "healthCheckPath": "/health",
      "envVars": [
        {
          "key": "NODE_ENV",
          "value": "production"
        },
        {
          "key": "PORT",
          "value": "10000"
        },
        {
          "key": "GOOGLE_CLIENT_ID",
          "value": "placeholder"
        },
        {
          "key": "GOOGLE_CLIENT_SECRET",
          "value": "placeholder"
        },
        {
          "key": "GOOGLE_REDIRECT_URI",
          "value": "https://datapingo-sheets-connector.onrender.com/auth/callback"
        },
        {
          "key": "SLACK_BOT_TOKEN",
          "value": "placeholder"
        },
        {
          "key": "SLACK_SIGNING_SECRET",
          "value": "placeholder"
        }
      ]
    }
  ]
}
